# AnyQB Environment Configuration
# Copy this file to .env and update with your values

# API Keys
ANTHROPIC_API_KEY=your-claude-api-key-here

# QuickBooks Configuration
QB_MCP_PATH=C:\Users\nando\Projects\anyQBMCP
QB_COMPANY_FILE=path-to-your-company-file

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
DEBUG_MODE=false

# Security
SECRET_KEY=your-secret-key-here
CORS_ORIGINS=["http://localhost:8000"]

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/anyqb.log

# Cache Settings
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000